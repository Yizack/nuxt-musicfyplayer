{
  "name": "nuxt-musicfyplayer",
  "version": "2.1.3",
  "description": "Embed a simple and beautiful HTML music player from local or hosted audio on your website using MediaElement.js and ColorThief.js",
  "keywords": [
    "nuxt",
    "nuxt3",
    "music-player",
    "music"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yizack/nuxt-musicfyplayer.git"
  },
  "homepage": "https://github.com/Yizack/nuxt-musicfyplayer",
  "author": {
    "name": "Yizack Rangel",
    "email": "yizackr@gmail.com",
    "url": "https://yizack.com"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\"",
    "test:types": "vue-tsc --noEmit",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.2",
    "colorthief": "2.4.0",
    "mediaelement": "^7.0.7"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.2",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^1.0.0",
    "@nuxt/schema": "^3.16.2",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "^22.14.0",
    "changelogen": "^0.6.1",
    "eslint": "^9.24.0",
    "nuxt": "^3.16.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "pnpm@10.7.1"
}
