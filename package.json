{
  "name": "nuxt-musicfyplayer",
  "version": "2.1.8",
  "description": "Embed a simple and beautiful HTML music player from local or hosted audio on your website using MediaElement.js and fast-average-color",
  "keywords": [
    "nuxt",
    "nuxt3",
    "music-player",
    "music"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yizack/nuxt-musicfyplayer.git"
  },
  "homepage": "https://github.com/Yizack/nuxt-musicfyplayer",
  "author": {
    "name": "Yizack Rangel",
    "email": "yizackr@gmail.com",
    "url": "https://yizack.com"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\"",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc -b --noEmit",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3",
    "fast-average-color": "^9.5.0",
    "mediaelement": "^7.0.7"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.8.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^24.2.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.32.0",
    "nuxt": "^4.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "packageManager": "pnpm@10.14.0"
}
